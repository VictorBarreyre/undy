{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "hushy",
   "plugins": [
  [
    "expo-notifications",
    {
      "icon": "./assets/notification-icon.png",
      "color": "#ffffff"
    }
  ],
  [
    "expo-location",
    {
      "locationAlwaysAndWhenInUsePermission": "Allow Hushy to use your location to find secrets around you.",
      "locationWhenInUsePermission": "Allow Hushy to use your location to find secrets around you."
    }
  ]
],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.hushy.app",
      "associatedDomains": [
        "applinks:hushy.app",
        "webcredentials:hushy.app"
      ],
      "infoPlist": {
        "CFBundleURLTypes": [{
          "CFBundleURLSchemes": ["hushy"]
        }]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hushy.android",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "hushy.app"
            },
            {
              "scheme": "hushy",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "deepLinking": {
        "schemes": ["hushy"]
      }
    }
  }
}